{
    "name": "xjimmy906/spout",
    "description": "基于box/spout的一个快速读写excel文件(csx,xlsx and ods)的php库(详细文档:https://opensource.box.com/spout/)",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "xjimmy906",
            "email": "xjw.1990@163.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "box/spout": "~3.1.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "xjimmy906\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    ]
}